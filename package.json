{
  "name": "acnh-guessing-game",
  "version": "1.0.0",
  "description": "Animal Crossing New Horizons Guessing Game with Enhanced Security",
  "main": "src/app.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/app.js",
    "dev:client": "vite --host",
    "dev:server": "nodemon src/app.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "fly deploy",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit",
    "test:watch": "NODE_ENV=test jest --watch --detectOpenHandles --forceExit",
    "test:coverage": "NODE_ENV=test jest --coverage --detectOpenHandles --forceExit",
    "test:logger": "node scripts/test-logger.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "seed": "node scripts/seed.js",
    "docs": "node scripts/generate-docs.js",
    "serve:docs": "npx http-server docs -o /docs/openapi.json"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.0",
    "moment-timezone": "^0.6.0",
    "mongodb": "^6.17.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "rate-limit-redis": "^3.0.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "concurrently": "^9.2.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "vite": "^7.0.2",
    "vite-plugin-static-copy": "^3.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Manfred-Klatt/Capstone-Project.git"
  },
  "keywords": [
    "animal-crossing",
    "game",
    "quiz",
    "authentication",
    "security"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Manfred-Klatt/Capstone-Project/issues"
  },
  "homepage": "https://github.com/Manfred-Klatt/Capstone-Project#readme"
}
